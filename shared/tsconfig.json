{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "types": [],
  },
  "include": ["src"],
  "exclude": ["src/**/*.native.ts", "src/**/*.native.tsx", "src/**/*.web.ts", "src/**/*.web.tsx"]
}
