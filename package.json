{
  "name": "journaling-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "shared",
    "web",
    "mobile"
  ],
  "scripts": {
    "web:dev": "npm run dev --workspace=@journaling-app/web",
    "web:build": "npm run build --workspace=@journaling-app/web",
    "mobile:start": "cd mobile && npx expo start",
    "mobile:ios": "npm run ios --workspace=@journaling-app/mobile",
    "mobile:android": "npm run android --workspace=@journaling-app/mobile"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "react": "^19.2.0"
  },
  "overrides": {
    "react-native": "0.76.9"
  },
  "devDependencies": {
    "@types/react": "^19.2.5",
    "typescript": "~5.9.3"
  }
}
